  <script defer data-site="YOUR_DOMAIN_HERE" src="https://api.nepcha.com/js/nepcha-analytics.js"></script>
    {% if error %}
        <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}
    <div class="row">
        <div class="col-md-10 col-lg-5 mx-auto d-flex flex-column justify-content-center vh-100">
            <form class="p-4 p-md-5 border rounded-3 bg-body-tertiary" action="{{ path('app_login') }}" method="post">
                <div class="form-floating mb-3">
                <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">
                <input autocomplete="off"  name="_username" type="text" class="form-control" id="floatingInput" placeholder="Nom d'utilisateur" value="{{ last_username }}">
                <label for="floatingInput">Nom d'utilisateur</label>
                </div>
                <div class="form-floating mb-3">
                <input type="password" class="form-control" id="password" placeholder="Mot de passe" name="_password">
                <label for="floatingPassword">Mot de passe</label>
                </div>
                <div class="checkbox mb-3">

                </div>
                <button class="w-100 btn btn-lg btn-success" type="submit">Connexion</button>
                <hr class="my-4">
                <a class="w-100 btn btn-lg btn-danger" href="{{ path('app_forgot_password_request') }}">Mot de passe oublié</a> <br><br>
                <a class="w-100 btn btn-lg btn-primary" href="{{ path('app_register') }}">S'enregistrer</a>
            </form>
        </div>
    </div>

<main class="main-content  mt-0">
    <div class="page-header align-items-start min-vh-100" style="background-image: url('https://images.unsplash.com/photo-1497294815431-9365093b7331?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1950&q=80');">
      <span class="mask bg-gradient-dark opacity-6"></span>
      <div class="container my-auto">
        <div class="row">
          <div class="col-lg-4 col-md-8 col-12 mx-auto">
            <div class="card z-index-0 fadeIn3 fadeInBottom">
              <div class="card-header p-0 position-relative mt-n4 mx-3 z-index-2">
                <div class="bg-gradient-primary shadow-primary border-radius-lg py-3 pe-1">
                  <h4 class="text-white font-weight-bolder text-center mt-2 mb-0">Sign in</h4>
                  <div class="row mt-3">
                    <!-- <div class="col-2 text-center ms-auto">
                      <a class="btn btn-link px-3" href="javascript:;">
                        <i class="fa fa-facebook text-white text-lg"></i>
